import java.util.*;
class Sample
{
int a=10;
static int b=20;
}
class TestC
{
public static void main(String args[])
{
Sample s1=new Sample();
System.out.println(s1.a+" "+s1.b);
Sample s2=new Sample();
System.out.println(s2.a+" "+s2.b);
s1.a=888;
s2.b=999;
System.out.println(s1.a+" "+s1.a);
System.out.println(s2.b+" "+s2.b);
}
}
