import java.io.*;
import java.util.*;

public class Solution 
{
public static void main(String[] args) 
{
 Scanner sc=new Scanner(System.in);
int n=sc.nextInt();
int r=n/4;
int r1=n%4;
if(n<100 || n>200){
System.out.println("INVALID INPUT");
}
    else
    {
 for(int i=1;i<4;i++)
 {
System.out.println(r);
 }
System.out.println(r+r1);
     }
  }
}
