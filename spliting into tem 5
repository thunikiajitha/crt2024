import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int a=s.nextInt();
        int b=s.nextInt();
        int r1=a/b;
        int r2=a%b;
        System.out.println("The number of students in each team is "+r1+" and left out is "+r2);
        
    }
}
