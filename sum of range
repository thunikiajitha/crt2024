import java.io.*;
import java.util.*;

public class Solution 
{

    public static void main(String[] args)
    {
        Scanner s=new Scanner(System.in);
        int min=s.nextInt();
        int max=s.nextInt();
        int sum=0;
        for(int i=min;i<=max;i++)
        {
            sum=sum+i;
    }
    System.out.println(sum);
    }
}
