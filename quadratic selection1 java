import java.io.*;
import java.util.*;

public class Solution {
public static void main(String args[]) {
        Scanner s=new Scanner(System.in);
        long x=s.nextLong();
        long y=s.nextLong();
        if(x>0 && y>0)
            System.out.println("1");
        if(x<0 && y>0)
            System.out.println("2");
        if(x<0 && y<0)
            System.out.println("3");
        if(x>0 && y<0)
            System.out.println("4");
        
    }
}
