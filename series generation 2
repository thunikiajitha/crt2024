import java.io.*;
import java.util.*;
public class Solution  
{
public static void main(String args[]) 
    {
   Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int a=7;
        for(int i=1;i<=n;i++)
        {
            System.out.print(a+" ");
            if(i%2!=0)
            {
                a=a-2;
            }
            else
            {
                a=a+3;
          }
       }
    }
}
